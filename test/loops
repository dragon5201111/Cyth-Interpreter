func myrec(n){
    if ((n == 0)){
        return nil
    }

    while ((true == true)){
        call print(n)
        break
        call print("this shouldn't be executed!")
    }

    call myrec((n - 1))
}

main {
    for (x=[1,2,34],,i = 0){
        break
    }
    # decl i = 0
    # while (true){
    #   return 21
    # }
    call myrec(5)
}